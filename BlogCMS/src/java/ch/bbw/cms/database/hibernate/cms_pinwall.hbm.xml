<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
   <class name="DBPinwall" table="cms_pinwall">
      <meta attribute="class-description">
         This class contains comments to posts 
      </meta>
      <id name="id" type="int" column="pin_id">
         <generator class="native"/>
      </id>
      <property name="postId" column="pin_post_id" type="int"/>
      <property name="userId" column="pin_user_id" type="int"/>

   </class>
</hibernate-mapping>